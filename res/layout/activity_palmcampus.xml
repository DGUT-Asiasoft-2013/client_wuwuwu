<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <fragment
        android:id="@+id/frag_tabbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        class="com.example.palmcampusmarket_client.fragment.MainTabbarFragment"
        tools:layout="@layout/fragment_page_home" />

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="471dp"
        android:layout_marginBottom="0dp" >
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frame_new_outside"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#55ffffff" >

        <fragment
            android:id="@+id/frag_new"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:layout_marginBottom="37dp"
            class="com.example.palmcampusmarket_client.collect.NewFragment"
            tools:layout="@layout/activity_new" />
    </FrameLayout>

</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.palmcampusmarket_client"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.palmcampusmarket_wallet.MyWalletActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.palmcampusmarket_wallet.MyBillActivity"/>
        <activity android:name="com.example.palmcampusmarket_wallet.RechargeActivity"/>
        <activity android:name=".HelloActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".CollectionsActivity"/>
        <activity android:name=".PasswordRecoverActivity"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".EvaluationActivity"/>
        <activity android:name=".NewCommodityActivity"/>
    </application>

</manifest>
